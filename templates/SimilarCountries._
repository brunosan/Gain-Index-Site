<table class='similar'>
  <tr class='header'>
    <th class='container'>Similar Countries</th>
    <th class='container'><div class='nowrap'><%= title%></div></th>
  </tr>
<% _.each(similar, function(country) { %>
  <tr id="country.id">
    <td class='container first'><a href='/country/<%= country['ISO3'] %>'><%= country.name %></a></td>
    <td class='container'><%= country.value %></td>
  </tr>
<% }); %>
</table>
