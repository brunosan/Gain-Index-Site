<div class='indicator-info container'>
  <a class='button green' href='/about/methodology#<%= methodologyHash %>'>More</a>
  <h3><%= title %></h3>
  <div class='description'>
    <p><%= description %></p>
  </div>
</div>
<div class='indicator-time container'>
  <div class='chart clearfix'>
    <h4><%= title %> over time </h4>
    <div class='graph'></div>
    <div class='legend'></div>
  </div>
</div>
<% if (source.length) { %>
<div class='source container'>
  <h4>Source</h4>
  <ul>
  <% _.each(source, function(source) { %>
      <li><a href='<%= source.link %>'><%= source.name %></a></li>
  <% }); %>
  </ul>
</div>
<% } %>
<div class='similar-countries'>
</div>
