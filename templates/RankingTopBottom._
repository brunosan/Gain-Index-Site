<table class='data ranktop'>
<thead>
<tr>
<th> Top 5 Countries </th>
<th> Score </th>
</tr>
</thead>
<tbody>
<% _.each(top, function(v, k) { %>
<tr>
<td> <div class='rank-number'><%= k+1 %></div> <span class='country-name'> <a href='/country/<%= v.ISO3 %>'> <%= v.name %> </a> </span> </td>
<td> <%= v.value %> </td>
</tr>
<% }); %>
</tbody>
</table>
<table class='data rankbottom'>
<thead>
<tr>
<th> Bottom 5 Countries </th>
<th> Score </th>
</tr>
</thead>
<tbody>
<% _.each(bottom, function(v, k) { %>
<tr>
<td> <div class='rank-number'><%= length - (4-k) %></div> <span class='country-name'> <a href='/country/<%= v.ISO3 %>'> <%= v.name %> </a> </span> </td>
<td> <%= v.value %> </td>
</tr>
<% }); %>
</tbody>
</table>
