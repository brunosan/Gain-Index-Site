<div class='country-profile'>
  <h2 class='container'><%= title %></h2>
  <div class='info container'>
    <div class='background-info left clearfix'><%= gdp.label%> (<%= gdp.year %>): <%= gdp.value %></div>
    <div class='background-info right clearfix'><%= population.label%> (<%= population.year %>): <%= population.value %></div>
  </div>

  <div class='country-summary clearfix'>
  </div>

  <div class='prose container'></div>

  <div class='rankings'>
  <% if (_(rank).size()) { %>
    <h3 class='container'>GaIn&#8482; Ranking since 1995</h3>
    <div class='ranking-history-wrapper'>
      <table class='data ranking-history'>
        <thead>
          <tr class='years'>
            <th class='label'>Year</th>
            <% _.each(rank, function(rank) { %>  <th><%= rank.year %></th>  <% }); %>
          </tr>
        </thead>
        <tbody>
          <tr>
            <th class='label'>Ranking</th>
            <% _.each(rank, function(rank) { %>  <td><%= rank.rank %></td>  <% }); %>
          </tr>
        </tbody>
      </table>
    </div>
  <% } %>
  </div>
  <div class='details'>
    <div class='tab-wrapper clearfix'>
      <ul class='tabs level-1'>
        <li><a href='#vulnerability' class='vulnerability'>Vulnerability</a></li>
        <li><a href='#readiness' class='readiness'>Readiness</a></li>
      </ul>
      <div class='tab-wrapper clearfix'>
        <ul class='tabs level-2'>
          <li><a href='#vulnerability/sector' class='vulnerability sectors'>Sector</a></li>
          <li><a href='#vulnerability/component' class='vulnerability components'>Component</a></li>
        </ul>
        <div class='bottomscorelabel label'>Denotes worst scores</div>
      </div>
    </div>
    <div id='tab' class='tab-content'>
      <table id='country-data' class='data full'></table>
    </div>
  </div>
</div>
