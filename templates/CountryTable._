<thead><tr>
    <th class='name'></th>
    <th class='graph'></th>
    <th class='raw'></th>
    <th class='normalized'>Score</th>
</tr></thead>
<tbody>
<% _.each(data, function(row) { %>
  <tr id='indicator-<%= row.field.id %>' class='<%= row.klass %>'>
    <td class='name'><a href='/ranking/<%= row.field.id %>' class='handle'><%= row.field.name %></a></td>
    <td class='graph'><div class='placeholder'></div></td>
    <td class='raw'><%= row.raw %></td>
    <td class='normalized'><%= row.normalized %></td>
  </tr>
<% }); %>
</tbody>
