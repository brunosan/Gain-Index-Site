<thead><tr>
    <th class='name label'>Indicator</th>
    <th class='graph label'>1995 to 2010</th>
    <th class='raw label'>Raw</th>
    <th class='normalized label'>Score</th>
</tr></thead>
<tbody>
<% _.each(data, function(row) { %>
  <% if (worst.hasOwnProperty(row.field.id)) { %>
    <tr id='indicator-<%= row.field.id %>' class='<%= row.klass %> worst'>
  <% } else { %>
    <tr id='indicator-<%= row.field.id %>' class='<%= row.klass %>'>
  <% } %>
    <td class='name'><%= row.field.name %>
      <% if (row.graph) { %>
        <div class='country-sector-graph'>
          <% _.each(row.graph, function(sector, name) { %>
            <% if (row.field.id == name) { %>
              <span class='sector-graph-<%= name %> hilite'
            <% } else { %>
              <span class='sector-graph-<%= name %>'
            <% } %>
            style='width:<%= sector.percent %>%;'>
            </span>
          <% }); %>
        </div>
      <% } %>
    </td>
    <td class='graph'><div class='placeholder'></div></td>
    <td class='input'><%= row.input %></td>
    <td class='score'><%= row.score %></td>
  </tr>
<% }); %>
</tbody>
