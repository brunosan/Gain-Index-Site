<thead><tr>
  <th><a class='sort rank' href='#sort-rank'>Rank</a></th>
  <th><a class='sort alpha' href='#sort-alpha'>Country</a></th>
  <th><a class='sort income' href='#sort-income'>Income group</a></th>
  <th></th>
</tr></thead>
<tbody>
  <% _.each(rows, function(v, k) { %>
    <tr id='country-<%= v.iso3 %>'>
      <td><div class='rank' style='background-color: #<%= v.color %>;'><%= v.rank %></div></div></td>
      <td><%= v.name %></td>
      <td><div class='income-group <%= v.incomeClass%>'><%= v.income%></div></td>
      <td><div class='graph'></div><%= v.score%></td>
    </tr>
  <% }); %>
</tbody>
