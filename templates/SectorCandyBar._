<h4><%= type %></h4>
<div class='country-sector-graph clearfix'>
  <% _.each(data, function(row, id) { %>
    <span class='sector-graph-<%= id %> hilite bar' style='width:<%= row.percent%>%;'>
      <label><%= row.name %> <%= row.percent %>% of <%=type %> score.</label>
    </span>
      <% }); %>
</div>
