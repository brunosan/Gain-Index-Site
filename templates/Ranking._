<div class='ranking'>
  <h2>Country Rankings</h2>
  <ul class='indices'>
    <% _.each(indices, function(index) { %>
      <li><a href='<%= index.path %>'><%= index.name %></a></li>
    <% }); %>
  </ul>
  <div class='categories'>
    <% if (active.path) { %>
    <ul>
      <li><a href='<%= active.path %>'><%= active.name %></a></li>
    </ul>
    <% } %>
	 	<ul>
      <% _.each(sectors, function(sector) { %>
        <li><a href='<%= sector.path %>'><%= sector.name %></a></li>
      <% }); %>
    </ul>
    <ul>
      <% _.each(components, function(component) { %>
        <li><a href='<%= component.path %>'><%= component.name %></a></li>
      <% }); %>
    </ul>
  </div>
  <table class='data'></table>
</div>
