<div class='ranking'>
  <h2>Country Rankings</h2>
  <ul class='indices'>
    <% _.each(indices, function(index) { %>
      <li><a href='/ranking/<%= index.id %>'><%= index.name %></a></li>
    <% }); %>
  </ul>
  <div class='categories'>
    <ul>
      <% _.each(sectors, function(sector) { %>
        <li><a href='/ranking/<%= sector.id %>'><%= sector.name %></a></li>
      <% }); %>
    </ul>
    <ul>
      <% _.each(components, function(component) { %>
        <li><a href='/ranking/<%= component.id %>'><%= component.name %></a></li>
      <% }); %>
    </ul>
  </div>
  <table class='data'></table>
</div>
