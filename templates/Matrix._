<div class='big-matrix'>
  <div class='tooltip'></div>
  <div class='graph'></div>
  <div class='year-controls'>
    <a href='#play' class='play-button'>Play</a>
    <ul class='year-selector'>
      <% for (var y= 1995; y <= 2010; y++) { %>
        <li><a href='#<%= y %>' class='year-<%= y %>'><%= y %></a></li>
      <% } %>
    </ul>
  </div>
</div>
<div class='country-selector' >
  <div>
    <input name='search' type='textfield' class='textfield' title='Add country to highlight' />
    <input name='add' type='button' class='button' value="Add" />
  </div>
  <div class='active-countries'></div>
</div>
<div class='gapminder'>
  <a href='http://bsan.eu/GaIn_GapMinder'>Compare more variables with Google</a>
